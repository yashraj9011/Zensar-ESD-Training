create table assignment(idno int auto_increment,
title varchar(200) not null,
start_date date,
primary key(idno));

create table trainee(
idno int primary key, 
tr_name varchar(200),
address varchar(300),
gender char(10),
dateofBirth date,
stipend float,
as_idno int,
foreign key(as_idno) references assignment(idno));


